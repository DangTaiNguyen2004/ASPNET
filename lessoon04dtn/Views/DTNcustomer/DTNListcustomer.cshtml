    @model List<lessoon04dtn.Models.DTNcustomer>
    @{
        ViewBag.Title = "danh sach khach hang";
        //var list = ViewBag.list;
        //var list = Model as List<lessoon04dtn.Models.DTNcustomer>;
        int stt = 0;
    }

    <h2>danh sach khach hang</h2>
    <div class=" row">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>#</th>
                    <th>ma so</th>
                    <th>ho va ten</th>
                    <th>dia chi</th>
                    <th>nam sinh</th>
                    <th>chuc nang</th>
                </tr>
            </thead>
            <tbody>
                @*@foreach (var item in list)*@
                @foreach (var item in Model)
                    {
                        stt++;
                        <tr>
                            <td>@stt</td>
                            <td>@item.CustomerId</td>
                            <td>@item.FirstName @item.LastName</td>
                            <td>@item.Address</td>
                            <td>@item.YearOfBirth</td>
                            <td>
                                <a href="#">Edit</a>
                                <a href="#">Delete</a>
                            </td>
                        </tr>
                    }
            </tbody>
        </table>
    </div>

